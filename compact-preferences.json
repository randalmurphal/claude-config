{
  "always_preserve": [
    "README.md",
    "package.json",
    "requirements.txt",
    "go.mod",
    "Cargo.toml",
    "pyproject.toml",
    "composer.json",
    "Gemfile",
    ".env.example",
    "**/CLAUDE.md"
  ],
  "keyword_boost": [
    "TODO",
    "FIXME",
    "HACK",
    "BUG",
    "SECURITY",
    "class",
    "function",
    "def",
    "interface",
    "type",
    "struct",
    "enum",
    "impl",
    "export",
    "import",
    "return",
    "async",
    "await",
    "useState",
    "useEffect"
  ],
  "file_pattern_boost": [
    "**/*.py",
    "**/*.js",
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    "**/*.go",
    "**/*.rs",
    "**/*.java",
    "**/*.cpp",
    "**/*.c",
    "**/*.h",
    "**/*.hpp",
    "**/*.cs",
    "**/*.rb",
    "**/*.php",
    "**/*.swift",
    "**/*.kt",
    "**/*.scala",
    "**/*.vue",
    "**/*.svelte"
  ],
  "error_keywords": [
    "AssertionError",
    "TypeError",
    "ReferenceError",
    "SyntaxError",
    "FAILED",
    "FAIL",
    "compilation failed",
    "build failed",
    "test failed",
    "undefined",
    "null pointer",
    "segmentation fault",
    "panic",
    "stack trace",
    "traceback"
  ],
  "max_context_lines": 500,
  "max_files_to_preserve": 75,
  "max_requirements": 30,
  "max_issues": 25,
  "code_preservation_rules": {
    "preserve_edited_files": true,
    "preserve_recent_edits_minutes": 30,
    "preserve_files_with_errors": true,
    "preserve_import_chains": true,
    "min_edit_count_to_preserve": 1
  },
  "removal_priority": [
    "tool_output_grep",
    "tool_output_ls",
    "tool_output_find",
    "repetitive_errors",
    "unchanged_file_reads",
    "duplicate_content"
  ]
}