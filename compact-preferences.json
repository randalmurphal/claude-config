{
  "always_preserve": [
    "**/CLAUDE.md",
    "README.md"
  ],
  "keyword_boost": [
    "TODO",
    "FIXME",
    "HACK",
    "BUG",
    "SECURITY",
    "NOTE",
    "class",
    "function",
    "def",
    "return",
    "async",
    "await"
  ],
  "file_pattern_boost": [
    "**/*.py",
    "**/*.js",
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    "**/*.go",
    "**/*.rs",
    "**/*.java",
    "**/*.cpp",
    "**/*.c",
    "**/*.h",
    "**/*.hpp",
    "**/*.cs",
    "**/*.rb",
    "**/*.php",
    "**/*.swift",
    "**/*.kt",
    "**/*.scala",
    "**/*.vue",
    "**/*.svelte"
  ],
  "error_keywords": [
    "AssertionError",
    "TypeError",
    "ReferenceError",
    "SyntaxError",
    "FAILED",
    "FAIL",
    "compilation failed",
    "build failed",
    "test failed",
    "undefined",
    "null pointer",
    "segmentation fault",
    "panic",
    "stack trace",
    "traceback"
  ],
  "max_context_lines": 1000,
  "max_files_to_preserve": 75,
  "max_requirements": 50,
  "max_issues": 40,
  "code_preservation_rules": {
    "preserve_edited_files": true,
    "preserve_recent_edits_minutes": 90,
    "preserve_files_with_errors": true,
    "preserve_import_chains": true,
    "min_edit_count_to_preserve": 1
  },
  "removal_priority": [
    "tool_output",
    "repetitive_errors",
    "unchanged_file_reads",
    "duplicate_content"
  ]
}